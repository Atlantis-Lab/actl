{
  "name": "@atlantis-lab/actl-cr-publisher",
  "repository": "git@github.com:Atlantis-Lab/actl.git",
  "version": "0.0.4",
  "license": "MIT",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "files": [
    "/bin",
    "/lib",
    "/oclif.manifest.json"
  ],
  "publishConfig": {
    "access": "public"
  },
  "oclif": {
    "bin": "actl",
    "commands": "./lib/commands"
  },
  "scripts": {
    "clean": "rm -r -f lib oclif.manifest.json",
    "prepublishOnly": "yarn run build",
    "build": "tsc && oclif-dev manifest readme",
    "prepack": "pubflow store",
    "postpack": "pubflow restore"
  },
  "dependencies": {
    "@actions/core": "1.2.0",
    "@actions/github": "1.1.0",
    "@atlantis-lab/actl-build": "^0.0.8",
    "@oclif/command": "^1",
    "@oclif/config": "^1",
    "@oclif/plugin-help": "2.2.2",
    "@octokit/graphql": "4.3.1",
    "@octokit/rest": "^16.15.0",
    "execa": "3.4.0",
    "tslib": "^1"
  },
  "devDependencies": {
    "@oclif/dev-cli": "^1",
    "@types/node": "14.0.14",
    "typescript": "3.9.6"
  }
}
