{
  "name": "@atlantis-lab/actl",
  "repository": "git@github.com:Atlantis-Lab/actl.git",
  "version": "0.4.1",
  "license": "MIT",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "bin": {
    "mctl": "./bin/run"
  },
  "files": [
    "/bin",
    "/lib",
    "/oclif.manifest.json"
  ],
  "publishConfig": {
    "access": "public"
  },
  "oclif": {
    "bin": "actl",
    "plugins": [
      "@oclif/plugin-help",
      "@oclif/plugin-plugins",
      "@atlantis-lab/config",
      "@atlantis-lab/actl-lint",
      "@atlantis-lab/actl-check",
      "@atlantis-lab/actl-typecheck"
    ]
  },
  "scripts": {
    "clean": "rm -r -f lib oclif.manifest.json",
    "prepublishOnly": "yarn run build",
    "build": "tsc && oclif-dev manifest readme",
    "prepack": "pubflow store",
    "postpack": "pubflow restore"
  },
  "dependencies": {
    "@atlantis-lab/actl-check": "0.4.1",
    "@atlantis-lab/actl-lint": "0.1.1",
    "@atlantis-lab/actl-typecheck": "0.1.1",
    "@atlantis-lab/config": "^0.1.2",
    "@oclif/command": "^1",
    "@oclif/config": "^1",
    "@oclif/plugin-help": "2.2.2",
    "@oclif/plugin-plugins": "1.7.8",
    "tslib": "^1"
  },
  "devDependencies": {
    "@monstrs/publish-flow": "^0.1.3",
    "@oclif/dev-cli": "^1",
    "@types/node": "14.0.14",
    "typescript": "3.9.6"
  }
}
